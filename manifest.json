    "background": {
        "page": "background.html"
    },
    "description": "The first extension that I made.",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "https://*/*",
        "notifications"
    ]
}
